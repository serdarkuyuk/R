beads <- rep(c("red","blue"),times=c(2,3))
sample(beads,1)
b <- 10000
events <- replicate(b,sample(beads,1))
tab <- table(events)
prop.table(tab)

events2 <- sample(beads, b, replace = TRUE)
prop.table(table(events2))

?set.seed
set.seed(1, sample.kind="Rounding")  #when using 3.6 R
mean(beads == "blue")
